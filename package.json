{
  "author": "Victor Figueiredo Mendes",
  "license": "MIT",
  "name": "tech-cart",
  "version": "2.0.0",
  "description": "Simple e-commerce application using angular",
  "type": "commonjs",
  "scripts": {
    "postinstall": "npm run copy-libs",
    "start": "http-server . -a localhost -p 7000 -c-1",
    "copy-libs": "cpx \"node_modules/{angular,angular-*}/**/*\" src/app/lib -C",
    "prestart": "npm install"
  },
  "dependencies": {
    "angular": "^1.8.2",
    "angular-route": "^1.8.3"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "http-server": "^14.1.1"
  }
}
