  "name": "tech-cart",
  "version": "2.0.0",
  "description": "Simple e-commerce application using angular",
  "type": "commonjs",
  "scripts": {
    "postinstall": "npm run copy-libs",
    "start": "http-server src/ -a localhost -p 7777 -c-1",
    "copy-libs": "cpx \"node_modules/{angular,angular-*}/**/*\" src/app/lib -C",
    "prestart": "npm install"
  },
  "dependencies": {
    "angular": "^1.8.2",
    "angular-route": "^1.8.3"
  },
  "devDependencies": {
    "angular-mocks": "^1.8.3",
    "cpx": "^1.5.0",
    "http-server": "^14.1.1",
    "jasmine-core": "^5.2.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-jasmine": "^5.1.0",
    "protractor": "^7.0.0"
  }
}
